<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="700.0" style="-fx-background-color: #9BD8BA;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="screen.NewGameController">
   <children>
      <Button fx:id="singleplay_btn" layoutX="211.0" layoutY="94.0" mnemonicParsing="false" onAction="#HandleSinglePlayer" prefHeight="43.0" prefWidth="278.0" style="-fx-background-color: #3FAC76;" text="SINGLE PLAYER" textFill="WHITE">
         <font>
            <Font name="Cambria Math" size="25.0" />
         </font>
      </Button>
      <Button fx:id="multLOCALi_btn" layoutX="211.0" layoutY="175.0" mnemonicParsing="false" onAction="#HandleLocalPlayers" prefHeight="43.0" prefWidth="278.0" style="-fx-background-color: #3FAC76;" text="LOCAL PLAYER" textFill="WHITE">
         <font>
            <Font name="Cambria Math" size="25.0" />
         </font>
      </Button>
      <Button mnemonicParsing="false" onAction="#back_click" prefHeight="32.0" prefWidth="64.0" style="-fx-background-color: #9BD8BA;" text="&lt;--" textFill="#1d804e">
         <font>
            <Font name="Cambria Math" size="21.0" />
         </font>
      </Button>
      <Button fx:id="Record_btn" layoutX="218.0" layoutY="348.0" mnemonicParsing="false" onAction="#RecordingAction" prefHeight="43.0" prefWidth="278.0" style="-fx-background-color: #3FAC76;" text="Recorded games" textFill="WHITE">
         <font>
            <Font name="Cambria Math" size="25.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
