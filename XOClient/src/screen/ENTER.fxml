<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="700.0" style="-fx-background-color: #9BD8BA;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="screen.ENTERController">
   <children>
      <Label fx:id="playerName" layoutX="51.0" layoutY="47.0" prefHeight="46.0" prefWidth="146.0" text="PLAYER NAME">
         <font>
            <Font name="Cambria Math" size="20.0" />
         </font>
      </Label>
      <Label fx:id="score" layoutX="540.0" layoutY="47.0" prefHeight="46.0" prefWidth="102.0" text="SCORE" textAlignment="CENTER">
         <font>
            <Font name="Cambria Math" size="20.0" />
         </font>
      </Label>
      <Button fx:id="newGame_txt" layoutX="252.0" layoutY="216.0" mnemonicParsing="false" onAction="#NewGameAction" prefHeight="46.0" prefWidth="196.0" style="-fx-background-color: #3FAC76;" text="NEW GAME">
         <font>
            <Font name="Cambria Math" size="26.0" />
         </font>
      </Button>
      <Button fx:id="record_txt" layoutX="252.0" layoutY="325.0" mnemonicParsing="false" onAction="#recordAction" prefHeight="46.0" prefWidth="196.0" style="-fx-background-color: #3FAC76;" text="RECORD">
         <font>
            <Font name="Cambria Math" size="25.0" />
         </font>
      </Button>
      <Button mnemonicParsing="false" onAction="#back_click" prefHeight="32.0" prefWidth="64.0" style="-fx-background-color: #9BD8BA;" text="&lt;--" textFill="#1d804e">
         <font>
            <Font name="Cambria Math" size="21.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
